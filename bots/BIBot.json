{
  "name": "BIBot",
  "description": "BIBot - a SQL query bot for the TICKIT database",
  "intents": [
        {
            "intentVersion": "$LATEST", 
            "intentName": "Hello_Intent"
        }, 
       
        {
            "intentVersion": "$LATEST", 
            "intentName": "Goodbye_Intent"
        },
		        {
            "intentVersion": "$LATEST", 
            "intentName": "Refresh_Intent"
        }, 
  ], 
  "locale": "en-US",
  "abortStatement": {
    "messages": [
      {
        "content": "Sorry, I could not understand. Goodbye.",
        "contentType": "PlainText"
      }
    ]
  },
  "childDirected": false,
  "clarificationPrompt": {
    "maxAttempts": 5,
    "messages": [
      {
        "content": "Come again?",
        "contentType": "PlainText"
      },
      {
        "content": "Sorry, can you please repeat that?",
        "contentType": "PlainText"
      }
    ]
  },
  "idleSessionTTLInSeconds": 300,
  "voiceId": "Matthew"
}
