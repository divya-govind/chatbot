{
	"name": "Count_Intent",
        "description": "Recursively query the dataset",
        "conclusionStatement": {
          "messages": [
            {
              "content": "via code hook",
              "contentType": "PlainText"
            }
          ]
        },
        "fulfillmentActivity": {
          "codeHook": {
            "messageVersion": "1.0",
            "uri": "{{lambda-arn}}"
          },
          "type": "CodeHook"
        },
        "sampleUtterances": [
			
		  "How many matches were played in the {time_year}",
		  "How many matches were played in the {venue_country}"
		  "How many matches were played on {time_weekday}"

        ],
    "slots": [
	{
            "slotType": "match_stage", 
            "name": "match_stage", 
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The name of the stage", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "slotTypeVersion": "$LATEST", 
            "sampleUtterances": [], 
            "description": "match_stage"
        }, 
		
		{
            "slotType": "Amazon.Date", 
            "name": "time_year", 
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The time", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "slotTypeVersion": "$LATEST", 
            "sampleUtterances": [], 
            "description": "time_year"
        }, 
		{
            "slotType": "Amazon.Country", 
            "name": "venue_country", 
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The time", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "slotTypeVersion": "$LATEST", 
            "sampleUtterances": [], 
            "description": "venue_country"
        }, 
		{
            "slotType": "Amazon.Date", 
            "name": "time_month", 
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The time", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "slotTypeVersion": "$LATEST", 
            "sampleUtterances": [], 
            "description": "time_month"
        }, 
		{
            "slotType": "Amazon.DayOfWeek", 
            "name": "time_weekday", 
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The time", 
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "slotTypeVersion": "$LATEST", 
            "sampleUtterances": [], 
            "description": "time_weekday"
        }
    ]
}
